<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="RegressionOnly" >
    <test name="Regression on Chrome">
        <parameter name="browser" value="chrome"/>
        <groups>
            <run>
                <include name="regression"/>
            </run>
        </groups>
        <classes>
            <class name="RegisterPageTestFactory"/>
        </classes>
    </test>
</suite>