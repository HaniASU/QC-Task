<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="CrossBrowserParallel" parallel="tests" thread-count="3">
    <test name="Chrome">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="RegisterPageTestFactory"/>
        </classes>
    </test>
    <test name="Edge">
        <parameter name="browser" value="edge"/>
        <classes>
            <class name="RegisterPageTestFactory"/>
        </classes>
    </test>
    <test name="Firefox">
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="RegisterPageTestFactory"/>
        </classes>
    </test>
</suite>